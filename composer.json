{
  "name": "camphi/lib-baseapiclient",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Philippe Mendes Campeau",
      "email": "campeau.philippe@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "ext-json": "*",
    "guzzlehttp/psr7": "^1.6.1|^2.0.0",
    "guzzlehttp/guzzle": "^6.5|^7.0",
    "php": ">=7.3"
  },
  "autoload": {
    "psr-4": {
      "Camphi\\BaseApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Camphi\\BaseApiClient\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "pestphp/pest": "2.x-dev",
    "mockery/mockery": "2.0.x-dev"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
